#!/usr/bin/env python3

# Rest of the code...

def factorize(n):
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return i, n // i
    return None

def main():
    import sys

    if len(sys.argv) != 2:
        print("Usage: ./factors <file>")
        sys.exit(1)

    with open(sys.argv[1], 'r') as file:
        numbers = [int(line.strip()) for line in file]

    for num in numbers:
        p, q = factorize(num)
        print(f"{num}={p}*{q}")

if __name__ == "__main__":
    main()
